(function (window, undefined) {	if (window.AHVP && window.AHVP.manager) {		window.AHVP.manager.render();		return;	};	if (window.hasAHVP) {		return;	};	var domscript = window.document.createElement("script");	domscript.src = "https://p-vp.autohome.com.cn/play/ahvp.player.2.4.5.js";	var head = document.getElementsByTagName("head")[0] || document.documentElement;head.insertBefore(domscript, head.firstChild );	window.hasAHVP = true;})(window);